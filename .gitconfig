[apply]
	whitespace = fix
[user]
	name = Severin Smith
	email = info@smallbutdigital.com
[color]
	ui = auto
	diff = auto
	status = auto
	branch = auto
[alias]
	br = branch
	ci = commit
	co = checkout
	st = status --short
	lg = log --oneline --decorate --graph --all
	unadd = reset HEAD
[github]
	user = sparks
[gist]
	browse = yes
	private = no
[merge]
	tool = opendiff
[push]
	default = matching
#	default = simple
[core]
	editor = /usr/bin/vim
